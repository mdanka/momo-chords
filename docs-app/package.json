    "@types/react-addons-shallow-compare": "^0.14.22",
    "@types/react-dom": "^16.0.7",
    "@types/react-router": "^4.0.31",
    "autoprefixer": "^6.6.0",
    "autoprefixer-loader": "^3.2.0",
    "css-loader": "^1.0.0",
    "exports-loader": "^0.7.0",
