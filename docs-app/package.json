    "momo-chords": "^0.2.4",
    "react": "^16.5.2",
    "react-addons-css-transition-group": "^15.6.2",
    "react-dom": "^16.5.2",
    "react-logger": "^1.1.0"
  },
  "devDependencies": {
    "@types/classnames": "^0.0.32",
    "@types/core-js": "^0.9.35",
    "@types/history": "^4.5.0",
