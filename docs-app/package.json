{
  "name": "momo-chords-docs",
  "private": true,
  "version": "0.1.0",
  "main": "index.js",
  "author": "Miklos Danka",
  "scripts": {
    "build": "npm-run-all clean createBuildDir -s bundle:prod",
    "buildDev": "npm-run-all clean createBuildDir -s bundle:dev ",
    "clean": "rimraf dist",
    "dev": "npm-run-all createBuildDir -p watch:server",
    "lint": "tslint -c tslint.json `find src -name '*.ts' -o -name '*.tsx'`",
    "lint-fix": "tslint --fix -c tslint.json `find src -name '*.ts' -o -name '*.tsx'`",
    "watch": "npm-run-all createBuildDir -p watch:bundle",
    "bundle:dev": "webpack -d",
    "bundle:prod": "webpack --config webpack.config.js",
    "createBuildDir": "mkdirp dist",
    "watch:bundle": "webpack -d -w",
    "watch:server": "webpack-dev-server -d --config webpack.config.server.js"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.4",
    "@fortawesome/free-solid-svg-icons": "^5.3.1",
